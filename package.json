{
  "name": "tl-djs",
  "version": "1.0.0",
  "description": "TL App for djs test case",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && node server.js",
    "build": "cross-env NODE_ENV=production webpack -p --color --progress",
    "build-dev": "webpack --env development --color --progress",
    "build-prod": "webpack -p --env production --color --progress",
    "watch": "webpack -w --color --progress",
    "serve": "webpack-dev-server --color",
    "srv": "webpack-dev-server  --env development --color",
    "up": "node server.js",
    "mon": "nodemon server.js",
    "debug": "node --inspect-brk=9229 server.js",
    "mondeb": "nodemon --inspect=9229 server.js"
  },
  "dependencies": {
